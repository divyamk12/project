echo "The first parameter is :$1"
echo "The second parameter is :$2"

sum=$(($1 + $2))

division=$(($1 / $2))

Multiplication=$(($1 * $2))

Subtraction=$(($1 - $2))

Modulus=$(($1 % $2))

echo The sum of 2 nos is :$sum
echo The remainder betwen the 2 nos is :$division
echo The multiplication of 2 nos is :$Multiplication
echo The Modulus of 2 nos is :$Modulus
echo "The parameters passed are :$@"
echo "The no of paramets passed are:$#"
echo "The process_id of operation is :$$"
echo "The exit status of prev cmmd is:$?"
echo "The process_id if daemon process is:$!"

